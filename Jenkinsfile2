node {
  stage ('scm-checkout') {
  git 'https://github.com/Sudheerkunda/my-app.git'
  }
  stage ('mvn-package') {
  sh 'mvn clean package'
  }
  stage ('tomcat-deploy'){
    sshagent(['tomcat']) {
    sh 'ssh -o StrictHostKeyChecking=no target/*.war  ec2-user@34.224.79.206:/apache-tomcat-10.0.6/webapps/'
}
}
}
